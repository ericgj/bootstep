[flake8]
max-complexity = 20
max-line-length = 88
select = E4,E7,E9,F
ignore = E402
exclude =
    .bzr,
    .direnv,
    .eggs,
    .git,
    .git-rewrite,
    .hg,
    .ipynb_checkpoints,
    .mypy_cache,
    .nox,
    .pants.d,
    .pyenv,
    .pytest_cache,
    .pytype,
    .ruff_cache,
    .svn,
    .tox,
    .venv,
    .vscode,
    __pypackages__,
    _build,
    buck-out,
    build,
    dist,
    node_modules,
    site-packages,
    venv,
    .venv-check
